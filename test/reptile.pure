namespace bvector;

using system;
using "bc:binvec-opt";

// macro to time (short) evaluations
def timex x = y,(t2-t1)/CLOCKS_PER_SEC when
                t1 = clock; 
                y = x; 
                t2 = clock;
              end;


let v1 = zvector;
timex (density v1);

let v2 = uvector;
timex (density v2);

timex (similarity v1 v2);

timex (adda v1 v2);

timex (similarity v1 v2);

timex (density v1);
timex (density v2);
